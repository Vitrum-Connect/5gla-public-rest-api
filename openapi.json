{
  "openapi": "3.0.1",
  "info": {
    "title": "5GLA Soil Scout Integration Service",
    "description": "This service provides the integration of the Soil Scout API with the 5GLA platform.",
    "version": "4.0.0"
  },
  "paths": {
    "/sensor-data-import/run": {
      "post": {
        "tags": [
          "sensor-data-import-controller"
        ],
        "description": "Run the import manually.",
        "operationId": "sensor-data-import.run",
        "responses": {
          "200": {
            "description": "The import was started."
          }
        }
      }
    },
    "/csv-sensor-data-import/": {
      "post": {
        "tags": [
          "csv-sensor-data-import-controller"
        ],
        "description": "Import sensor data from CSV.",
        "operationId": "csv-sensor-data-import.import",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CsvSensorDataImportRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "The import has been started."
          }
        }
      }
    },
    "/info/version": {
      "get": {
        "tags": [
          "info-controller"
        ],
        "description": "Fetch the version of the application.",
        "operationId": "info.version",
        "responses": {
          "200": {
            "description": "The version of the application.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/VersionResponse"
                }
              }
            }
          }
        }
      }
    },
    "/info/last-run": {
      "get": {
        "tags": [
          "info-controller"
        ],
        "description": "Fetch the last run of the import.",
        "operationId": "info.last-rum",
        "responses": {
          "200": {
            "description": "The last run of the application.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LastRunResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CsvSensorDataImportRequest": {
        "type": "object",
        "properties": {
          "base64EncodedCsvData": {
            "type": "string",
            "description": "The base64 encoded CSV data."
          },
          "decodedCsvData": {
            "type": "string"
          }
        },
        "description": "CSV data import request."
      },
      "VersionResponse": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "string",
            "description": "The timestamp of the response."
          },
          "version": {
            "type": "string",
            "description": "The version."
          }
        },
        "description": "Response wrapper."
      },
      "LastRunResponse": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "string",
            "description": "The timestamp of the response."
          },
          "lastRun": {
            "type": "string",
            "description": "The last run."
          }
        },
        "description": "Response wrapper."
      }
    }
  }
}